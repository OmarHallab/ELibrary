<%= form_for(@reading_list) do |f| %>
  <div class="field">
    <%= f.label :user_id %><br>
    <%= f.text_field :user_id %>
  </div>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= label_tag :article %><br>
    <%= select_tag "reading_list[article]", options_from_collection_for_select(@articles, "id", "title") %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
